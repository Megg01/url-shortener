generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model ShortLink {
  id        String   @id @default(cuid())
  longUrl   String
  shortCode String   @unique
  createdAt DateTime @default(now())
  clicks    Int      @default(0)
}
